{
  "root": "tools/docs-generator/common",
  "sourceRoot": "tools/docs-generator/common",
  "projectType": "library",
  "targets": {
    "html": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "npx compodoc -p tools/docs-generator/tsconfig.docs.json --output=dist/docs/"
          }
        ]
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["npx http-server dist/docs -p 4401"]
      }
    }
  },
  "tags": ["type:tool"]
}
