{
  "root": "tools/dep-graph",
  "sourceRoot": "tools/dep-graph",
  "projectType": "library",
  "targets": {
    "html": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "nx dep-graph --file=dist/dep-graph/index.html"
          }
        ]
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["npx http-server dist/dep-graph -p 4401"]
      }
    }
  },
  "tags": ["type:tool"]
}
