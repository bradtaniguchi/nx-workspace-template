{
  "version": 2,
  "projects": {
    "common": "libs/common",
    "gh-codespaces": "libs/gh-codespaces"
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "strict": true,
      "standaloneConfig": true
    },
     "@nrwl/node:library": {
      "linter": "eslint",
      "unitTestRunner": "jest",
      "strict": true,
      "standaloneConfig": true
    },
    "@nrwl/angular:app": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "strict": true,
      "standaloneConfig": true
    },
    "@nrwl/angular:component": {
      "style": "scss",
      "changeDetection": "OnPush"
    }
  }
}
